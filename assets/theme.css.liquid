/* DROP DOWN MENU */



#table-lamp-drop-nav{
	top: 85px; 
	left: 80px;
}

#table-lamp-drop-nav, #projects-drop-nav {
	position: absolute; /*--Important--*/
	z-index: 1000;
	padding: 10px 10px 10px;
	float: left;
	background-color: #eee;
	border: solid 1px #aaa;
	font-size: 0.8em;
	
	/*--Bottom left rounded corner--*/
	-moz-border-radius: 10px;
	-khtml-border-radius: 10px;
	-webkit-border-radius: 10px;
	/*--Hidden for those with js turned off--*/
	display: none;
	width: 520px;
	
	-moz-box-shadow: 5px 5px 5px black;
	-webkit-box-shadow: 5px 5px 5px black;
	-khtml-box-shadow: 5px 5px 5px black;
	 box-shadow: 10px 5px 5px black;
		 
}
ul#links li .row { /*--If needed to break out into rows--*/
	clear: both;
	float: left;
	width: 100%;
	margin-bottom: 10px;
}


#table-lamp-drop-nav ul{
	list-style: none;
	width: 250px;
	float: left;
}
#table-lamp-drop-nav ul li {
	width: 100%; /*--Override parent list item--*/
	color: #fff;
	height: auto;
}

#table-lamp-drop-nav ul li h2 { /*--Sub nav heading style--*/
	font-size: 1em;
	font-weight: normal;
	height: auto;
	margin-top: 0px;
	margin-bottom: 5px;
	background-color: #ccc;
	margin-right: 10px;
	padding: 3px;
	padding-left: 5px;
	-moz-border-radius: 3px;
	-khtml-border-radius: 3px;
	-webkit-border-radius: 3px;
}
#table-lamp-drop-nav ul li h2 a { /*--Sub nav heading link style--*/
	background-image: none;
	color: #000;
	background: none;
	text-align: left;
	width: 225px;
	height: auto;
}
#table-lamp-drop-nav ul li a {
	
	float: none;
	text-indent: 0; /*--Override text-indent from parent list item--*/
	height: auto; /*--Override height from parent list item--*/
	display: block;
	text-decoration: none;
	color: #000;
	background: none;
	text-align: left;
	width: 225px;

	margin: 0px;
	margin-bottom: 5px;
	padding: 0px;
	padding-left: 5px;
}
#table-lamp-drop-nav ul li a:hover {
	color: #666;
	background-position: 5px 12px ;/*--Override background position--*/
}

#table-lamp-dots{
	top: 47px; 
	left: 80px;
	width: 520px;
}

.dots{
